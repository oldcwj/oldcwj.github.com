---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
<title>What's on my mind: Collantes.US</title>
<link href="http://log.collantes.us/atom.xml" rel="self"/>
<link href="http://log.collantes.us/"/>
<updated>{{ site.time | date_to_xmlschema }}</updated>
<id>http://log.collantes.us/</id>
<author>
<name>David Collantes</name>
<email>david@collantes.us</email>
</author>
{% for post in site.posts limit:13 %}
<entry>
<title>{{ post.title }}</title>
<link href="http://log.collantes.us{{ post.url }}"/>
<updated>{{ post.date | date_to_xmlschema }}</updated>
<id>http://log.collantes.us{{ post.id }}</id>
<content type="html">{{ post.content | xml_escape }}</content>
</entry>
{% endfor %}
</feed>
